      <font>
         <Font name="SansSerif Regular" size="18.0" />
      </font>
   </Text>
   <Button fx:id="button1" layoutX="236.0" layoutY="153.0" mnemonicParsing="false" onAction="#messoption" prefHeight="25.0" prefWidth="129.0" text="Order from Mess" textAlignment="JUSTIFY" />
   <Button fx:id="button2" layoutX="236.0" layoutY="219.0" mnemonicParsing="false" onAction="#calcandpayfeesoption" prefHeight="25.0" prefWidth="129.0" text="Calculate &amp; Pay Fees" textAlignment="JUSTIFY" />
